    "lightweight-charts": "^5.0.8",
    "lucide-react": "^0.525.0",
    "next": "14.2.5",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-financial-charts": "^2.0.1",
    "react-hot-toast": "^2.5.2",
    "recharts": "^3.1.0"
  },
  "devDependencies": {
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "autoprefixer": "^10.4.21",
    "eslint": "^8",
    "eslint-config-next": "14.2.5",
